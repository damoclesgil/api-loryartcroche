{
  "collectionName": "components_cores_component_colors",
  "info": {
    "displayName": "Color",
    "icon": "paint",
    "description": ""
  },
  "options": {},
  "attributes": {
    "nomeDaCor": {
      "type": "enumeration",
      "enum": [
        "Rosa",
        "Azul",
        "Vermelho",
        "Branco",
        "Laranja",
        "Verde"
      ]
    },
    "produto_referentes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::produto.produto"
    },
    "cor": {
      "type": "customField",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "customField": "plugin::color-picker.color"
    }
  }
}
